<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itboyst.facedemo.mapper.StudentOuttimeMapper">

    <resultMap type="com.itboyst.facedemo.domain.StudentOuttime" id="StudentOuttimeResult">
        <result property="idcard" column="idcard" javaType="java.lang.Integer"/>
        <result property="name" column="name" javaType="String"/>
    </resultMap>

    <insert id="insertStudentOuttime" parameterType="com.itboyst.facedemo.domain.StudentOuttime">
        insert into student_outtime(idcard, name)
        VALUES (#{idcard}, #{name})
    </insert>

</mapper>